<div class='output-page'>
  <div class='o-form'>
      <h1>アウトプット投稿</h1>

      <%= form_with model: @output, local: true do |f| %>
        <div>
          <span>タイトル</span>
        </div>
          <%= f.text_field :title, class: "o-title" %>
        <div>
          <span>本文</span>
        </div>
        <%= f.text_area :text, class: "o-text" %>
        <div>
          <%= f.submit '投稿する' %>
        </div>
      <% end %>
  </div>
</div>